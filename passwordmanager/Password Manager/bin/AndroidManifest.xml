<?xml version="1.0" encoding="UTF-8"?>
<manifest android:versionCode="1" android:versionName="1.0"
    package="com.example.password.manager" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="15"/>
    <application android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/title_activity_main" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <provider
            android:authorities="com.example.password.manager.database.providers.passwordscontentprovider" android:name="com.example.password.manager.database.providers.PasswordsContentProvider"/>
        <provider
            android:authorities="com.example.password.manager.database.providers.configurationcontentprovider" android:name="com.example.password.manager.database.providers.ConfigurationContentProvider"/>
        <activity android:name=".passworddetails.PasswordDetails" android:label="Password Details"></activity>
    </application>
</manifest>
